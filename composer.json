{
    "name": "riipandi/laravel-encryptdb",
    "description": "Encrypt/decrypt stored records in database using eloquent",
    "keywords": ["laravel", "eloquent", "encrypt", "decrypt", "models", "database"],
    "type": "library",
    "require": {
        "php": ">=7.1",
        "laravel/framework": "^6.0"
    },
    "license": "Apache-2.0",
    "authors": [{
        "name": "Aris Ripandi",
        "email": "aris@ripandi.id"
    }],
    "autoload": {
        "psr-4": {
            "Riipandi\\LaravelEncryptDb\\": "src/"
        }
    },
    "autoload-dev":{
        "psr-4": {
            "Riipandi\\LaravelEncryptDb\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">5.0"
    }
}
